# mpv addons
[submodule "mpv/.config/mpv/scripts/videoclip"]
	path = mpv/.config/mpv/scripts/videoclip
	url = https://github.com/Ajatt-Tools/videoclip
[submodule "mpv/.config/mpv/scripts/youtube-quality/remote"]
	path = mpv/.config/mpv/scripts/youtube-quality/remote
	url = https://github.com/jgreco/mpv-youtube-quality
[submodule "mpv/.config/mpv/scripts/live-filters/remote"]
	path = mpv/.config/mpv/scripts/live-filters/remote
	url = https://github.com/hdb/mpv-live-filters
[submodule "mpv/.config/mpv/scripts/zenity-open-files"]
	path = mpv/.config/mpv/scripts/zenity-open-files
	url = https://github.com/alifarazz/mpv-zenity-open-files
# Ranger plugins
[submodule "ranger/.config/ranger/plugins/ranger-archives"]
	path = ranger/.config/ranger/plugins/ranger-archives
	url = https://github.com/maximtrp/ranger-archives.git
[submodule "mpv-youtube-ui"]
	path = mpv/.config/mpv/scripts/youtube-ui/remote
	url = https://github.com/eatsu/mpv-osc-youtube-ui
[submodule "yt-dlp-streamtape-plugins"]
	path = yt-dlp/.config/yt-dlp/plugins/yt-dlp-streamtape-plugins
	url = https://github.com/impulse888/yt-dlp-streamtape-plugins
[submodule "mpv-ui"]
	path = mpv/.config/mpv/scripts/ui/remote
	url = https://github.com/tomasklaen/uosc
